{
  "semi": false,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "es5",
  "tailwindPreserveWhitespace": true,
  "plugins": ["prettier-plugin-tailwindcss"],
  "printWidth": 100,
  "arrowParens": "always",
